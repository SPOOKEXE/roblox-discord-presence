	end
	local Last : string = "Z"
	local Count : number = 1
	for i=1, #Numeral do
		local Numeral : string = string.sub(Numeral, i, i)
		if not RomanDigit[Numeral] then
			return nil
		end
		if Numeral == Last then
			Count = Count + 1
			if Count >= 4 then
